{
  "name": "nopcommerce-e2e-playwright",
  "version": "1.0.0",
  "description": "End-to-end test automation for nopCommerce demo using Playwright and TypeScript",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "npx playwright test",
    "test:auth": "npx playwright test tests/auth.spec.ts",
    "test:headed": "npx playwright test --headed",
    "test:debug": "npx playwright test --debug",
    "report:html": "npx playwright show-report",
    "report:allure": "allure generate allure-results --clean -o allure-report && allure open allure-report"
  },
  "keywords": ["playwright", "e2e", "testing", "nopcommerce", "typescript"],
  "author": "Aleksa AleksiÄ‡",
  "license": "ISC",
  "devDependencies": {
    "@playwright/test": "^1.55.0",
    "@types/node": "^24.5.2",
    "allure-commandline": "^2.34.1",
    "allure-playwright": "^3.4.1",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-playwright": "^2.2.2",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  }
}
